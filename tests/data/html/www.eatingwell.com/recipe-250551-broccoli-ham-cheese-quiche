<section class="recipeIngredients" ng-controller="ew_controllers_recipe_ingredient" ng-init="init(6, 250551, 'Recipe')">

    <h2 class="headingH2GuttersIngredients">
    Ingredients
    <span class="recipeIngredientsHeaderToggles">
        <meta id="metaRecipeServings" itemprop="recipeYield" content="6">

        

        
        <a href="javascript:void(0)" id="servings-button" style="cursor: default;">
            
