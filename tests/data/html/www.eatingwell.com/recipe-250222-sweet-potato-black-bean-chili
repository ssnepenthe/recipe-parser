                        <span itemprop="ingredients">4 cloves garlic, minced</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">2 tablespoons chili powder</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">4 teaspoons ground cumin</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">½ teaspoon ground chipotle chile (see Note)</span>
                    </li>

        </ul>
        <ul class="multiColumn listIngredients clearfix" ng-hide="ingredientsReloaded">
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">¼ teaspoon salt</span>
                    </li>
